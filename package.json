{
  "name": "frameworks",
  "version": "1.2.1",
  "homepage": "https://frameworks.now.sh/",
  "private": true,
  "dependencies": {
    "bulma": "^0.5.1",
    "classnames": "^2.2.5",
    "font-awesome": "^4.7.0",
    "gh-pages": "^1.0.0",
    "history": "^4.7.2",
    "husky": "^0.14.3",
    "lint-staged": "^4.0.1",
    "prettier": "^1.5.2",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "devDependencies": {
    "enzyme": "^2.9.1",
    "jest-serializer-enzyme": "^1.0.0",
    "react-scripts": "1.0.10",
    "react-test-renderer": "^15.6.1"
  },
  "jest": {
    "snapshotSerializers": [
      "jest-serializer-enzyme"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --print-width 100 --write",
      "git add"
    ]
  },
  "scripts": {
    "pretty": "prettier --single-quote --print-width 100 --write \"src/**/*.{js,jsx,json,css}\"",
    "precommit": "lint-staged",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "now": "npm run build && now build --public --name frameworks"
  }
}
